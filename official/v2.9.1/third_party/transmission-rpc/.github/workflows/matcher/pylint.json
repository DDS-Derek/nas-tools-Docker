{
  "problemMatcher": [
    {
      "owner": "pylint",
      "fileLocation": [
        "relative",
        "${workspaceFolder}"
      ],
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+):\\s+[RCWE].+:\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    }
  ]
}
